<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#00000000" />
            <stroke
                android:width="2dp"
                android:color="#243743" />
        </shape>
    </item>
    <item
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp">
        <shape android:shape="oval">
            <gradient android:startColor="#363A42"
                android:endColor="#DB5800"/>

        </shape>
    </item>
</layer-list>